#include <stdio.h>
#include <stdlib.h>

int main()
{
    float sb,sl,ip;
    printf("informe o salario bruto: ");
    scanf("%f",&sb);

if (sb < 1000.0)
    {
     ip = sb * 0.05;
    } else{
          if(sb < 5000.0){
            ip = sb * 0.11;
          }else{
            ip = sb * 0.35;
          }
    }
    sl = sb - ip;
printf("O salario liquido foi de: %.2f \n",sl);

printf("O imposto foi de: %.2f \n",ip);

printf("O salario Bruto foi de: %.2f \n",sb);
}
